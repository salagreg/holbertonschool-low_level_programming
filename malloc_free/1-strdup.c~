#include <stdio.h>
#include "main.h"
#include <stdlib.h>
/**
 *
 *
 *
 *
 */
char *_strdup(char *str)
{
	int i;
	int j;
	char *newstr;
	
	if (str == NULL)
		return (NULL);

	for (i = 0; str[i] != '\0'; i++)

		char newstr = malloc(i + 1);

	if (newstr == NULL)
		return (NULL);

	for (j = 0; j < i; j++)
	{
		newstr[i] = str[j];
	}
	return (newstr);
}
