#include <stdio.h>
#include "main.h"
/**
* main - fonction principale
* print_alphabet : avoir les lettres de l'alphabet en minuscule
* Return: 0
*/
void print_alphabet(void) /** variable pour avoir l'alphabet **/
{
	char letter;

	for (letter = 'a' ; letter <= 'z' ; letter++)
	{
		_putchar(letter);
	}
	_putchar('\n');
}
