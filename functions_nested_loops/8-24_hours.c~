#include "main.h"
#include <stdio.h>
/**
* jack_bauer - affiche heures & minutes
* void : neant
* Return: ne retourne rien car argument 'void'
*/
void jack_bauer(void)
{
  int heure;
  int minute;

  for (heure = 0 ; heure <= 23 ; heure++)
    {
      for (minute = 0 ; minute <= 59 ; minute++)
	{
	  _putchar((heure / 10) + '0');
	  _putchar((heure % 10) + '0');
	  _putchar(':');
	  _putchar((minute / 10) + '0');
	  _putchar((minute % 10) + '0');
	  _putchar('\n');
	}
    }
  _putchar('\n');
}
